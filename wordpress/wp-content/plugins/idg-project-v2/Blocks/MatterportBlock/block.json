{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "idg/matterport-block",
	"title": "Matterport",
	"category": "embed",
	"icon": "embed-generic",
	"description": "Embed Matterport files",
	"keywords": [ "matterport", "walkthrough", "embed", "idg" ],
	"supports": {
		"html": false,
		"align": true
	},
	"attributes": {
		"matterport_link": {
			"type": "string",
			"default": "https://my.matterport.com/show/?m=t7zY1VXgpB6"
		},
		"width": {
			"type": "integer",
			"default": 1076
		},
		"height": {
			"type": "integer",
			"default": 900
		}
	},
	"editorScript": "file:../../build/index.js"
}
